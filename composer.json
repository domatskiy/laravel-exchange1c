{
    "name": "domatskiy/laravel-exchange1c",
    "type": "library",
    "description": "Loader from 1c - CommerceML",
    "keywords": ["1c", "CommerceML", "laravel"],
    "homepage": "https://github.com/domatskiy",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeniy Domatskiy",
            "role": "Developer"
        }
    ],
    "repositories":[
        {
            "type":"git",
            "url":"https://github.com/domatskiy/laravel-exchange1c"
        },
        {
            "type":"package",
            "package":{
                "name":"domatskiy/laravel-exchange1c",
                "version":"0.9.0",
                "source":{
                    "type":"git",
                    "url":"https://github.com/domatskiy/laravel-exchange1c",
                    "reference":"master"
                }
            }
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Domatskiy\\": "src/" }
        },
    "autoload-dev": {
        "psr-4": {
            "Domatskiy\\Tests\\": "tests/"
            }
        },
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "^5.4",
        "illuminate/support": "^5.0,<5.6",
        "illuminate/console": "^5.0,<5.6",
        "illuminate/filesystem": "^5.0,<5.6",
        "barryvdh/reflection-docblock": "^2.0.4",
        "symfony/class-loader": "^2.3|^3.0"
        },
    "require-dev": {
        "phpunit/phpunit": "^5.4.0",
        "illuminate/config": "^5.0,<5.6",
        "doctrine/dbal": "~2.3"
        }
}
